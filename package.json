{
    "name": "Multipage",
    "version": "1.0.1",
    "description": "A multipage project based on webpack4",
    "main": "index.js",
    "scripts": {
        "serve": "npm run dev --m serve",
        "build": "npm run pro --m build",
        "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",

        "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.conf.js ",
        "pro": "cross-env webpack --config config/webpack.prod.conf.js"
    },
    "author": "Patric",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/plugin-transform-runtime": "^7.7.6",
        "@babel/preset-env": "^7.7.7",
        "autoprefixer": "^9.7.3",
        "babel-loader": "^8.0.6",
        "babel-polyfill": "^6.26.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^6.0.3",
        "css-loader": "^3.4.0",
        "exports-loader": "^0.7.0",
        "file-loader": "^5.0.2",
        "glob": "^7.1.6",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^2.1.5",
        "purify-css": "^1.2.5",
        "purifycss-webpack": "^0.7.0",
        "sass": "^1.24.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.3",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.7.7",
        "core-js": "^3.6.2"
    },
    "browserslist": [
        "defaults",
        "not ie < 11",
        "last 2 versions",
        "> 1%",
        "iOS 7",
        "last 3 iOS versions"
    ]
}
